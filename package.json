{
    "name": "rpg-js",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "staticFiles": {
        "staticPath": "static",
        "watcherGlob": "**"
    },
    "alias": {
        "core": "./core",
        "server": "./server",
        "rpg": "./rpg",
        "mmo": "./mmo"
    },
    "scripts": {
        "start:rpg": "parcel -p 3000 rpg/index.html",
        "start:mmo": "parcel -p 3001 mmo/index.html",
